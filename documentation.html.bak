  <script>
    // Documentation navigation functionality
    function initDocNav() {
      const docLinks = document.querySelectorAll('.doc-link');
      const sideNavBtns = document.querySelectorAll('.side-nav-btn');
      const sideNav = document.getElementById('sideNav');
      const sideNavToggle = document.getElementById('sideNavToggle');
      const sideNavFab = document.getElementById('sideNavFab');
      const allNavElements = [...docLinks, ...sideNavBtns];
      
      // FAB button functionality - Open nav
      if (sideNavFab) {
        sideNavFab.addEventListener('click', function(e) {
          e.preventDefault();
          sideNav.classList.remove('hidden');
          sideNav.classList.add('visible');
        });
      }
      
      // Toggle button inside nav - Close nav
      if (sideNavToggle) {
        sideNavToggle.addEventListener('click', function(e) {
          e.preventDefault();
          sideNav.classList.add('hidden');
          sideNav.classList.remove('visible');
        });
      }
      
      // Navigation link functionality
      allNavElements.forEach(link => {
        link.addEventListener('click', function(e) {
          e.preventDefault();
          
          const sectionId = this.getAttribute('data-section');
          if (!sectionId) return;
          
          // Hide all sections
          const sections = document.querySelectorAll('.doc-section');
          sections.forEach(section => {
            section.classList.remove('active');
          });
          
          // Show selected section
          const selectedSection = document.getElementById(sectionId);
          if (selectedSection) {
            selectedSection.classList.add('active');
          }
          
          // Update active link styling for desktop sidebar
          docLinks.forEach(l => {
            l.classList.remove('active');
          });
          const matchingDocLink = document.querySelector(`.doc-link[data-section="${sectionId}"]`);
          if (matchingDocLink) {
            matchingDocLink.classList.add('active');
          }
          
          // Update active button styling for mobile side nav
          sideNavBtns.forEach(btn => {
            btn.classList.remove('active');
          });
          this.classList.add('active');
          
          // Close nav on mobile after selection
          if (window.innerWidth <= 768) {
            sideNav.classList.add('hidden');
            sideNav.classList.remove('visible');
          }
          
          // Scroll to top of main content smoothly
          document.querySelector('main').scrollIntoView({ behavior: 'smooth', block: 'start' });
        });
      });
    }
    
    // Initialize on page load
    document.addEventListener('DOMContentLoaded', initDocNav);
  </script>
